{
	"name": "lopshop",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run",
		"db:push": "drizzle-kit push",
		"db:migrate": "drizzle-kit migrate",
		"db:studio": "drizzle-kit studio",
		"db:seed": "tsx scripts/seed.ts",
		"db:seed:postgres": "node --experimental-specifier-resolution=node scripts/db/seed-postgres.js",
		"prisma:seed": "node --experimental-specifier-resolution=node prisma/seed.js",
		"db:init": "prisma migrate reset --force && npm run prisma:seed"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@lucide/svelte": "^0.486.0",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/adapter-cloudflare": "^7.0.2",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.16",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.4",
		"@types/node": "^22.14.0",
		"@types/uuid": "^10.0.0",
		"autoprefixer": "^10.4.20",
		"bits-ui": "^1.3.17",
		"clsx": "^2.1.1",
		"drizzle-kit": "^0.30.2",
		"embla-carousel-autoplay": "^8.5.2",
		"embla-carousel-svelte": "^8.5.2",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^2.46.1",
		"formsnap": "^2.0.0",
		"globals": "^15.14.0",
		"jsdom": "^25.0.1",
		"lucide-svelte": "^0.476.0",
		"mode-watcher": "^0.5.1",
		"postcss": "^8.5.3",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prisma": "^6.6.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"svelte-sonner": "^0.3.28",
		"sveltekit-superforms": "^2.24.1",
		"tailwind-merge": "^3.0.2",
		"tailwind-variants": "^0.3.1",
		"tailwindcss": "^3.4.17",
		"tailwindcss-animate": "^1.0.7",
		"ts-node": "^10.9.2",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.0.0",
		"vitest": "^3.0.0",
		"wrangler": "^4.14.1",
		"zod": "^3.24.2"
	},
	"dependencies": {
		"@azure/identity": "^4.7.0",
		"@fontsource-variable/inter": "^5.1.1",
		"@fontsource-variable/onest": "^5.1.1",
		"@fontsource/inter": "^5.1.1",
		"@fontsource/onest": "^5.1.1",

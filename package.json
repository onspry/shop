		"@melt-ui/svelte": "^0.86.3",
		"@microsoft/microsoft-graph-client": "^3.0.7",
		"@microsoft/microsoft-graph-types": "^2.40.0",
		"@node-rs/argon2": "^2.0.2",
		"@oslojs/crypto": "^1.0.1",
		"@oslojs/encoding": "^1.1.0",
		"@prisma/client": "^6.6.0",
		"arctic": "^3.3.0",
		"drizzle-orm": "^0.39.3",
		"light": "^2.0.0-alpha.26"
	}
}
